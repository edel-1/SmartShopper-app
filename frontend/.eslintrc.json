{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "off",
    //"quotes": ["error", "double"],
    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-vars": "error",
    "react/jsx-uses-react": "error",
    //"no-unused-vars": ["error", "varsIgnorePattern", "React"]
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^h$"
      }
    ]
  }
}
